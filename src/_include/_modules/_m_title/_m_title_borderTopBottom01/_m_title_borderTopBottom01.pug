mixin m-title-borderTopBottom01(tg)
	-
		var op = {
			tag:       tg.tag ? tg.tag : "div",
			baseName:  tg.baseName ? tg.baseName : "m-title-borderTopBottom01",
			className: tg.className ? tg.className : "",
			text:      tg.text ? tg.text : "ダミータイトルです"
		}

	#{op.tag}( class = op.baseName+op.className )
		span!= op.text