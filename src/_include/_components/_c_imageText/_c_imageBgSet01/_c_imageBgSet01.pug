//- mixin
mixin c-imageBgSet01(tg)
	-
		var op = {
			tag:       tg.tag ? tg.tag : "div",
			baseName:  tg.baseName ? tg.baseName : "c-imageBgSet01",
			className: tg.className ? tg.className : "is-left",
			before: tg.before ? tg.before : false,
			after: tg.after ? tg.after : false,
		}
	
	#{op.tag}( class = op.baseName + " " + op.className )
		block
		if op.before == true
			.group.is-before
				+beforeGroup()
		.group.is-primary
			+primaryGroup()
		if op.after == true
			.group.is-after
				+afterGroup()
		
