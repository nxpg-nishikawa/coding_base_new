extends /_include/_template/_t_base_template

block vars
	- var KEY = "component"
	- var URL = 'component.html'
	- var RELATIVE_PATH = ''
	- var TITLE = 'タイトル'
	- var DESCRIPTION = ''
	- var KEYWORDS = ''
	- var header = "false"
	- var footer = "false"
	- var side = ["false","left"]
	- var jquery = "true"

block contents
	+l-contentsWidth({tag:"div"})
		+l-mqWrapper({tag:"div"})

			//- 繰り返しのとき
			+c-btnSet01({tag:"div", colClass:"is-two",
				list:[{tag:"p",text:"ボタン01", url:"index.html", className:"is-icon"},
					{tag:"p",text:"ボタン02", url:"index.html"},
					{tag:"p",text:"ボタン03", url:"index.html"}
				]
			})

			//- 個別にタグ指定したいとき
			+c-btnSet01({tag:"div", className: "is-center", colClass:"is-two"})
				li
					+m-btn01({tag:"p",text:"ボタン01", url:"index.html"})
				li
					+m-btn01({tag:"p",text:"ボタン02", url:"index.html"})
				li
					+m-btn01({tag:"p",text:"ボタン02", url:"index.html"})